# Ruby Dev
alias b="bundle --jobs 4"
alias be="bundle exec"

# Git
alias g="git"
alias gsco="stash_and_checkout"
alias grebasesafe="git fetch --tags && git rebase last-successful-build"
alias gg="git grep"

# System
alias cc="clear"
alias ll="ls -l"
alias all="ls -al"
alias llf="ls -al | fzf"

# Directory Navigation
alias ..="cd .."
alias ...="cd ../.."
alias dot="cd ~/dotfiles"

# For cli programs
alias scli="scala-cli"
